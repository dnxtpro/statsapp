{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/lawea",
          "devCommand": "npm run start",
          "buildCommand": "npm run build:prod",
          "outputDirectory": "dist/lawea"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  